    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback"
  },
  "keywords": [],
  "author": "Jacob Shannon and Alex Whan",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "knex": "^0.21.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "redis": "^3.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.0.27",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.25",
    "@types/uuid": "^8.0.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  }
}